<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="358.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="Controller.Login">
   <TextField fx:id="userNameText" layoutX="168.0" layoutY="128.0" prefHeight="40.0" prefWidth="327.0">
      <font>
         <Font size="18.0"/>
      </font>
   </TextField>
   <Label alignment="CENTER" layoutX="60.0" layoutY="129.0" prefHeight="38.0" prefWidth="108.0" text="Username"
          textAlignment="CENTER" textFill="#0c7604">
      <font>
         <Font name="System Bold" size="18.0"/>
      </font>
   </Label>
   <TextField fx:id="passwordText" layoutX="168.0" layoutY="184.0" prefHeight="40.0" prefWidth="327.0">
      <font>
         <Font size="18.0"/>
      </font>
   </TextField>
   <Label alignment="CENTER" layoutX="60.0" layoutY="185.0" prefHeight="38.0" prefWidth="108.0" text="Password"
          textAlignment="CENTER" textFill="#0c7604">
      <font>
         <Font name="System Bold" size="18.0"/>
      </font>
   </Label>
   <Label alignment="CENTER" layoutX="114.0" layoutY="63.0" prefHeight="38.0" prefWidth="370.0"
          text="Fictional Global Consulting Company" textAlignment="CENTER" textFill="#0c7604">
      <font>
         <Font name="System Bold" size="18.0"/>
      </font>
   </Label>
   <Button layoutX="372.0" layoutY="244.0" mnemonicParsing="false" onAction="#loginButtonAction" text="Login"
           textFill="#0c7604">
      <font>
         <Font name="System Bold" size="14.0"/>
      </font>
   </Button>
   <Label alignment="CENTER" layoutX="1.0" layoutY="319.0" prefHeight="38.0" prefWidth="108.0" text="ZoneId"
          textAlignment="CENTER" textFill="#0c7604">
      <font>
         <Font name="System Bold" size="14.0"/>
      </font>
   </Label>
   <Label fx:id="errorMessage" alignment="CENTER" layoutX="4.0" layoutY="279.0" prefHeight="38.0" prefWidth="590.0"
          text="Error Message" textAlignment="CENTER" textFill="#ea1a0b" visible="false">
      <font>
         <Font name="System Bold" size="14.0"/>
      </font>
   </Label>
   <Button layoutX="449.0" layoutY="244.0" mnemonicParsing="false" onAction="#exitButtonAction" text="Exit"
           textFill="#0c7604">
      <font>
         <Font name="System Bold" size="14.0"/>
      </font>
   </Button>
</AnchorPane>
