<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="358.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.Login">
   <TextField fx:id="userNameText" layoutX="168.0" layoutY="128.0" prefHeight="40.0" prefWidth="398.0">
      <font>
         <Font size="18.0" />
      </font>
   </TextField>
   <Label fx:id="usernameLabel" alignment="CENTER_RIGHT" layoutX="-2.0" layoutY="129.0" prefHeight="38.0" prefWidth="163.0" text="\%username" textAlignment="CENTER" textFill="#0c7604">
      <font>
         <Font name="System Bold" size="18.0" />
      </font>
   </Label>
   <TextField fx:id="passwordText" layoutX="168.0" layoutY="184.0" prefHeight="40.0" prefWidth="398.0">
      <font>
         <Font size="18.0" />
      </font>
   </TextField>
   <Label fx:id="passwordLabel" alignment="CENTER_RIGHT" layoutX="-2.0" layoutY="185.0" prefHeight="38.0" prefWidth="163.0" text="\%password" textAlignment="CENTER" textFill="#0c7604">
      <font>
         <Font name="System Bold" size="18.0" />
      </font>
   </Label>
   <Label fx:id="companyLabel" layoutX="166.0" layoutY="63.0" prefHeight="38.0" prefWidth="422.0" text="\%companyName" textAlignment="CENTER" textFill="#0c7604">
      <font>
         <Font name="System Bold" size="18.0" />
      </font>
   </Label>
   <Button fx:id="loginButton" layoutX="307.0" layoutY="244.0" mnemonicParsing="false" onAction="#loginButtonAction" prefHeight="30.0" prefWidth="108.0" text="\%login" textFill="#0c7604">
      <font>
         <Font name="System Bold" size="14.0" />
      </font>
   </Button>
   <Label fx:id="zoneIDLabel" alignment="CENTER" layoutY="319.0" prefHeight="38.0" prefWidth="600.0" text="ZoneId" textAlignment="CENTER" textFill="#0c7604">
      <font>
         <Font name="System Bold" size="14.0" />
      </font>
   </Label>
   <Label fx:id="errorMessage" alignment="CENTER" layoutX="4.0" layoutY="279.0" prefHeight="38.0" prefWidth="590.0" text="Error Message" textAlignment="CENTER" textFill="#ea1a0b" visible="false">
      <font>
         <Font name="System Bold" size="14.0" />
      </font>
   </Label>
   <Button fx:id="exitButton" layoutX="458.0" layoutY="244.0" mnemonicParsing="false" onAction="#exitButtonAction" prefHeight="30.0" prefWidth="108.0" text="\%exit" textFill="#0c7604">
      <font>
         <Font name="System Bold" size="14.0" />
      </font>
   </Button>
   <ImageView fx:id="logoImage" fitHeight="66.0" fitWidth="81.0" layoutX="74.0" layoutY="49.0" pickOnBounds="true" preserveRatio="true" />
</AnchorPane>
